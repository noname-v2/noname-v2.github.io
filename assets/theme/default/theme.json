	"card": {
		"width": "105px",
		"ratio": "1.4",
		"background": "@(card.webp)",
		"back-background": "@(cardback.webp)",
		"entry-background": "@(wood.webp)",
		"radius": "8px",
		"shadow": "none",
		"text-shadow": "rgb(237, 232, 219) 0 0 2px, rgb(237, 232, 219) 0 0 2px, rgb(237, 232, 219) 0 0 2px, rgb(237, 232, 219) 0 0 2px, rgb(237, 232, 219) 0 0 2px, rgb(237, 232, 219) 0 0 2px",
