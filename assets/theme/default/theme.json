	"pop": {
		"width": "85px",
		"scale": "0.567",
		"margin": "8",
		"tray-height": "60px",
		"tray-margin": "16",
		"timer-background": "rgba(0, 0, 0, 0.2)",
		"timer-bar-background": "rgb(231, 116, 50)",
