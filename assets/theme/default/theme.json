{
	"app": {
		"text-shadow": "black 0 0 2px",
		"color": "white",
		"font": "system-ui",
		"caption-font": "lishu",
		"label-font": "xingkai",
		"background": "@(background.webp) center/cover",
		"transition": "0.3s",
		"transition-fast": "0.2s",
		"transition-slow": "0.5s",
		"transition-faster": "0.1s",
		"transition-slower": "0.7s",
		"blurred-opacity": "0.2",
		"defer-opacity": "0.5",
		"zoom-scale": "0.8",
		"splash-margin": "13px"
	},

	"arena": {
		"player-width": "150px",
		"player-height": "210px",
		"card-width": "105px",
		"card-height": "147px"
	},

	"button": {
		"font": "xingkai",
		"radius": "100%",
		"background": "@(button.webp)",
		"image-opacity": "0.5",
		"shadow": "rgba(0, 0, 0, 0.4) 0 0 0 1px, rgba(0, 0, 0, 0.2) 0 3px 10px",
		"fill-shadow": "rgba(0, 0, 0, 0.6) 0 0 0 1px, rgba(0, 0, 0, 0.2) 0 3px 10px"
	},

	"player": {
		"width": "150px",
		"ratio": "1.4",
		"background": "rgba(0, 0, 0, 0.4)",
		"back-background": "rgba(0, 0, 0, 0.4)",
		"radius": "8px",
		"shadow": "rgba(0, 0, 0, 0.4) 0 0 0 1px, rgba(0, 0, 0, 0.2) 0 3px 10px",
		"back-shadow": "rgba(0, 0, 0, 0.4) 0 0 0 1px, rgba(0, 0, 0, 0.2) 0 3px 10px",
